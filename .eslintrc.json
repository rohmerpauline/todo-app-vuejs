{
  "env": {
    "node": true
  },
  "overrides": [
    {
      "files": ["*.vue", "*.js", "*.jsx", "*.cjs", "*.mjs"],
      "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended",
        "@vue/eslint-config-prettier/skip-formatting"
      ]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  }
}
